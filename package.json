  "author": "alberto gozalez novella <alberto.gonzalez@gft.com> Juan Pedro Martínez <juanpedro.martinez@gft.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "server": "webpack-dev-server --open --port 3333 --content-base dist/",
    "api": "nodemon ./api/server.js --exec babel-node --presets babel-preset-env",
    "dev": "webpack --config=webpack.dev.js --watch",
    "start": "concurrently \"npm run dev\" \"npm run server\"",
    "build": "webpack --optimize-dedupe --optimize-occurrence-order --optimize-minimize --config=webpack.prod.js",
    "prod": "webpack --config=webpack.prod.js --watch"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "chalk": "^2.4.0",
    "clean-webpack-plugin": "^0.1.19",
    "compression-webpack-plugin": "^1.1.11",
    "express": "^4.16.3",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "ng-annotate-loader": "^0.6.1",
    "nodemon": "^1.17.3",
    "pug": "^2.0.3",
    "pug-html-loader": "^1.1.5",
    "pug-loader": "^2.4.0",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "angular": "^1.6.10"
  }
}